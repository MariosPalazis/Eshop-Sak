{{#if products}}
<article class="product-id-container">
    {{#each products}}
    <p class="product-id" id="{{this.productID}}" >{{this.productID}}</p>
    {{/each}}
</article>
<article class="wheel-diameter-container" >
    {{#each products}}
    <p class="wheel-diameter {{this.wheel_diameter.unit}}" id="{{this.wheel_diameter.amount}}">{{this.wheel_diameter.amount}}</p>
    {{/each}}
</article>
<article class="stock-container" >
    {{#each products}}
    <p class="stock" id="{{this.stock}}"
    <p class=""
</article>
{{/if}

<article class="single-dimension" id="wd">
            <div class="dimension-circle" ><div class="circle" id="wheel-diam-circle"></div></div>
                <div class="vars">
                    <label class="varName">WHEEL DIAMETER</label></br>
                    <label class="varValue" id="wheel-diam-value">Choose an option</label>
                    <div class="options" id="wdo">
                        <div class="option"><label class="value" id="wheel-diam-val1"> >57 cm (22")</label></div>
                        <hr>
                        <div class="option"><label class="value" id="wheel-diam-val2">58-86 cm (23-34")</label></div>
                        <hr>
                        <div class="option"><label class="value" id="wheel-diam-val3">87-114 cm (34-45")</label></div>
                        <hr>
                        <div class="option"><label class="value" id="wheel-diam-val4">115-143 cm (45-56")</label></div>
                        <hr>
                        <div class="option"><label class="value" id="wheel-diam-val5">144-172 cm (56-67")</label></div>
                        <hr>
                        <div class="option"><label class="value" id="wheel-diam-val6">173-190 cm (67-74")</label></div>
                    </div>
            </div>
</article>
